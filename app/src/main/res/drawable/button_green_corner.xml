<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape android:shape="rectangle"  >
            <corners android:radius="30px" />
            <stroke android:width="1dip" android:color="@color/colorGreenPrimary" />
            <gradient android:angle="-90" android:startColor="#4ABF1B"
                android:endColor="#4ABF1B"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30px" />
            <stroke android:width="1dip" android:color="@color/colorGreenPrimary" />
            <gradient android:angle="-90" android:startColor="@color/colorGreenPrimary" android:endColor="@color/colorGreenPrimary" />
        </shape>
    </item>
</selector>